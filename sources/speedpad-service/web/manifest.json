{
  "manifest_version": 2,
  "key": "",
  "minimum_chrome_version": "23",

  "name": "Chrome Speedpad Service",
  "description": "A Service-Helper App for the Chrome Speedpad Extension.",
  "version": "0.0.1.0",

  "app": {
    "background": {
      "scripts": ["background.dart.js"]
    }
  },

  "icons": { "16": "css/icons/service_icon_16.png", "128": "css/icons/service_icon_128.png" },

  "permissions": [
    "syncFileSystem",
    "fileSystem",
    "storage",
    "unlimitedStorage"
  ]
}